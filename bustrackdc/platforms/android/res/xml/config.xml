<?xml version='1.0' encoding='utf-8'?>
<widget id="org.JasonRosenbaum.WheresTheBusDC" version="1.8.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="AdmobPlugin">
        <param name="android-package" value="com.admob.cordova.plugin.AdmobAdPlugin" />
    </feature>
    <feature name="UniversalAnalytics">
        <param name="android-package" value="com.danielcwilson.plugins.analytics.UniversalAnalyticsPlugin" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" value="com.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="MapKit">
        <param name="android-package" value="com.phonegap.plugins.mapkit.MapKit" />
    </feature>
    <name>BusTrackDC</name>
    <description>
        A bus tracking app for the Washington, DC metro rail and bus and circulator system.
    </description>
    <author email="seminal@theseminal.com" href="http://bustrackdc.com">
        Jason Rosenbaum
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
